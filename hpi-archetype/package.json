{
  "name": "@artifactId@",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "jjsbuilder",
    "test": "jjsbuilder --tasks test",
    "mvnbuild": "jjsbuilder --tasks bundle",
    "mvntest": "jjsbuilder --tasks test,lint",
    "lint": "jjsbuilder --tasks lint",
    "bundle": "jjsbuilder --tasks bundle",
    "bundle:watch": "jjsbuilder --tasks bundle:watch",
    "postinstall": "npm install npm && node postinstall.js",
    "do_prod_installs": "npm install --save @jenkins-cd/design-language @jenkins-cd/js-extensions @jenkins-cd/js-modules @jenkins-cd/sse-gateway",
    "do_dev_installs": "npm install --save-dev @jenkins-cd/js-builder @jenkins-cd/js-test"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^2.8.0",
    "eslint-plugin-react": "^5.0.1",
    "gulp": "^3.9.1"
  },
  "dependencies": {
    "react": "15.1.0",
    "react-dom": "15.1.0"
  },
  "jenkinscd": {
    "import": [
      "@jenkins-cd/sse-gateway@any"
    ]
  }
